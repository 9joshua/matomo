{% extends '@Login/loginLayout.twig' %}

{% set title %}{{ 'TwoFactorAuth_TwoFactorAuthentication'|translate }}{% endset %}

{% block loginContent %}
    <div
        vue-entry="TwoFactorAuth.LoginTwoFactorAuth"
        form-data="{{ formData|json_encode|e('html_attr') }}"
        access-error-string="{{ accessErrorString|default(null)|json_encode|e('html_attr') }}"
        form-nonce="{{ formNonce|json_encode|e('html_attr') }}"
        login-module="{{ loginModule|json_encode|e('html_attr') }}"
        piwik-url="{{ piwikUrl|default(null)|json_encode|e('html_attr') }}"
        user-login="{{ userLogin|json_encode|e('html_attr') }}"
        contact-email="{{ contactEmail|json_encode|e('html_attr') }}"
    ></div>
{% endblock %}